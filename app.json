{
  "expo": {
    "name": "event-explorer",
    "slug": "event-explorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "eventexplorer",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "Enable Face ID to quickly and securely sign in to Event Explorer."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_TICKETMASTER_API_KEY": "f2G2ePCKAcsEY4Iy9OfJDKbdHUqqWQHd",
      "eas": {
        "projectId": ""
      },
      "firebase": {
        "apiKey": "AIzaSyAhh09LdJrqUYV9aEfcvU7PMtPTCJho-_Y",
        "authDomain": "event-explorer-local.firebaseapp.com",
        "projectId": "event-explorer-local",
        "storageBucket": "event-explorer-local.firebasestorage.app",
        "messagingSenderId": "112222412976",
